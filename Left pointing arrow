      <option>Lefttexttop</option>
      <option>Lefttextdown</option>
    </property>
    
     <configurations>
      <configuration name="Lefttexttop" value="Type:Lefttexttop" implements="lefttexttop" />
      <configuration name="Lefttextdown" value="Type:Lefttextdown" implements="lefttextdown" />
