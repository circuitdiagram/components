<?xml version="1.0" encoding="utf-8"?>
<component version="1.5" xmlns="http://schemas.circuit-diagram.org/circuitDiagramDocument/2012/component/xml">
  <declaration>
    <meta name="name" value="Solenoid" />
    <meta name="minsize" value="40" />
    <meta name="author" value="briannicholls" />
    <meta name="additionalinformation" value="http://www.circuit-diagram.org/" />
    <meta name="org.circuit-diagram.bundled" value="true" />

    <property name="Type" type="enum" default="open" serialize="t" display="Type">
      <option>Normally Open</option>
      <option>Normally Closed</option>
    </property>

    <configurations>
      <configuration name="Solenoid (Open)" value="Type:open" />
      <configuration name="Solenoid (Closed)" value="Type:closed" />
    </configurations>
  </declaration>
  <connections autorotate="HorizontalToVertical">
    <connection name="a" start="_Start" end="_Start-13y" edge="_Start-13y" />
    <connection name="b" start="_End-6x" end="_End-13y-6x" edge="_End-13y-6x" />
  </connections>
  <render autorotate="HorizontalToVertical">
    <g conditions="$Type==open" autorotate="HorizontalToVertical,FlipSecondary">
      <path start="_Middle-12x" data="M0,0 c0,-15 18,-15 18,0" />
      <path start="_Middle-12x" data="M0,0 c0,15 -18,15 -18,0" />
      <path start="_Middle-12x" data="M36,0 c0,15 -18,15 -18,0" />
      <line start="_Start" end="_Start-13y" />
      <line start="_End-6x" end="_End-13y-6x" />
      <rect x="_Middle-38" y="_Middle-5" width="70" height="11" mask="url(#stripe)" />
    </g>
  </render>
</component>
