    <meta name="org.circuit-diagram.bundled" value="true" />

    <property name="Value" type="string" default="5" serialize="value" display="Value">
    </property>

    <property name="Type" type="enum" default="Righttexttop" serialize="t" display="Type">
      <option>Righttexttop</option>
      <option>Righttextdown</option>
    </property>
    
    <configurations>
      <configuration name="Righttexttop" value="Type:Righttexttop" implements="Righttexttop" />
      <configuration name="Righttextdown" value="Type:Righttextdown" implements="Righttextdown" />
    </configurations>
 
  </declaration>
  	<connections>
      	<group conditions="$Type(eq_Righttexttop)">
      		<connection name="com" start="_Start" end="_End" edge="both" />
    	</group>
      
    	<group conditions="$Type(eq_Righttextdown)">
      		<connection name="com" start="_Start" end="_End" edge="both" />
    	</group>
      
    </connections>
  <render>

    <!-- Righttexttop -->
    <group conditions="$Type(eq_Righttexttop),_Horizontal">
      <path start="_End-5y" data="l 0,10 l 10,-5, l -10,-5" />
      <text value="$Value" x="_Middle" y="_Middle-15" align="CentreRight" size="large" />
    </group>
    <group conditions="$Type(eq_Righttexttop),!_Horizontal">
      <line start="_Start5x" end="_Start" />
      <path start="_Start5x-5y" data="l 0,10 l 10,-5, l -10,-5" />
      <text value="$Value" x="_Start-6" y="_Middle" align="CentreRight" size="large" />
    </group>
    <group conditions="$Type(eq_Righttexttop)">
      <line start="_Start" end="_End" />
    </group>
    
    <!-- Righttextdown -->
    <group conditions="$Type(eq_Righttextdown),_Horizontal">
      <path start="_End-5y" data="l 0,10 l 10,-5, l -10,-5" />
      <text value="$Value" x="_Middle" y="_Middle+15" align="CentreRight" size="large" />
    </group>
    <group conditions="$Type(eq_Righttextdown),!_Horizontal">
      <line start="_Start5x" end="_Start" />
      <path start="_Start5x-5y" data="l 0,10 l 10,-5, l -10,-5" />
      <text value="$Value" x="_Start+6" y="_Middle" align="CentreLeft" size="large" />
    </group>
    <group conditions="$Type(eq_Righttextdown)">
      <line start="_Start" end="_End" />
    </group>
    
  </render>
